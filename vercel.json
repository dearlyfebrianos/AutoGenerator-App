{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/apk/(.*)",
      "destination": "/apk/$1"
    },
    {
      "source": "/ipa/(.*)",
      "destination": "/ipa/$1"
    },
    {
      "source": "/desktop/(.*)",
      "destination": "/desktop/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}